<h1>Listing student_qualifications</h1>

<table>
  <tr>
    <th>Student</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @student_qualifications.each do |student_qualification| %>
  <tr>
    <td><%= student_qualification.student_id %></td>
    <td><%= link_to 'Show', student_qualification %></td>
    <td><%= link_to 'Edit', edit_student_qualification_path(student_qualification) %></td>
    <td><%= link_to 'Destroy', student_qualification, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

